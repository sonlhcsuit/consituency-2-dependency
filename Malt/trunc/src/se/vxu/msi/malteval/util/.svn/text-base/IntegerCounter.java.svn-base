package se.vxu.msi.malteval.util;

/**
 * @author Jens Nilsson (jens.nilsson@msi.vxu.se)
 */
public class IntegerCounter {
	private int parsedCorrect, parsedTotal, gsCorrect, gsTotal;
	
	public IntegerCounter() {
		parsedCorrect = 0;
		parsedTotal = 0;
		gsCorrect = 0;
		gsTotal = 0;
	}
	
	public int getGsCorrect() {
		return gsCorrect;
	}
	public void incGsCorrect() {
		gsCorrect++;
	}
	public int getGsTotal() {
		return gsTotal;
	}
	public void incGsTotal() {
		gsTotal++;
	}
	public int getParsedCorrect() {
		return parsedCorrect;
	}
	public void incParsedCorrect() {
		parsedCorrect++;
	}
	public int getParsedTotal() {
		return parsedTotal;
	}
	public void incParsedTotal() {
		parsedTotal++;
	}
}
